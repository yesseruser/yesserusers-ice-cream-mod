{
  "schema_version": 1,
  "quilt_loader": {
    "group": "${group}",
    "id": "yesserusers_ice_cream_mod",
    "version": "${version}",
    "metadata": {
      "name": "Yesseruser's Ice Cream Mod",
      "description": "A simple ice cream mod. Adds multiple ice creams that can be crafted and eaten.",
      "contributors": {
        "yesseruser": "Owner"
      },
      "icon": "assets/yesserusers_ice_cream_mod/textures/item/sweet_berry_ice_cream.png",
      "license": "MIT"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "cz.yesseruser.quilt.ExampleModQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.20"
      },
      {
        "id": "architectury",
        "version": "*"
      }
    ]
  }
}
